import java.util.Scanner;
public class APCodingNinja {
    public static void main(String[] args)
    {
        Scanner sc = new Scanner(System.in);
        int n = sc.nextInt();
        int count=1,current=1,i;
        while (count<=n)
        {
             int num=3*current+2;
             if(num%4!=0)
             {
                 System.out.println(num + " ");
                 count++;
             }
             current++;

        }
        }
    }
