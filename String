public class StringBasic {
    public static void main(String[] args) {
 
       String str = "Hello Umang";
       String str1 = "This is Software Engineering";
       String str2 = "Hello Umang";
       String str3 = "Never Give Up";
       System.out.println(str.charAt(4));
       System.out.println(str +" "+ str1);
       System.out.println(str1 + " "+str2);
       System.out.println(str1.equals(str2));
       System.out.println(str1.concat(str).length());
       System.out.println(str3.substring(0,5).length());
       System.out.println(str3.contains("  Give Up"));
       System.out.println(str.compareTo(str2));

    }
}
